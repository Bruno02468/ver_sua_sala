<script>
var webservice_url = 'https://atividades.educacionalweb.com.br/EducacionalWebAPI';
//var webservice_url = 'http://localhost:31477';




function atualizarNotificacoesUsuario(token, escola)
{
	
	var successAtualizarNotificacoesUsuario = function(data){
		var msg_novas = 0;
		mensagens = data.lista;

		for(i=0; i<mensagens.length; i++)
		{ 
			msg_novas += mensagens[i].qtde;
		};

		$j("#notif-prof").removeClass("notif-on");  
		$j("#notif-prof").removeClass("notif-off");
		
		$j("#notif").html(msg_novas);  
		if (msg_novas == 0) { classNotif = 'notif-off' } else { classNotif = 'notif-on' }
		$j("#notif-prof").addClass(classNotif);  
	}

	$j.ajax({
		type: "GET",
		url: webservice_url + "/Mensagens/listarTurmas/json",
		dataType: 'json',
		data: { token: token, escola:escola, turma:"", codDis:"", subDis: "" },
		success: successAtualizarNotificacoesUsuario
	});
	
}
</script>




